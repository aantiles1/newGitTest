<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
   <modules jcr:primaryType="jnt:modules">
      <template-tutorial j:dependencies="bootstrap4-components bootstrap4-core default popperjs"
                         j:installedModules="template-tutorial"
                         j:modulePriority="0"
                         j:moduleType="templatesSet"
                         j:title="template-tutorial"
                         jcr:primaryType="jnt:module">
         <portlets jcr:primaryType="jnt:portletFolder"/>
         <files jcr:primaryType="jnt:folder"/>
         <contents jcr:primaryType="jnt:contentFolder"/>
         <templates jcr:primaryType="jnt:templatesFolder">
            <files jcr:primaryType="jnt:folder">
               <themes jcr:primaryType="jnt:folder">
                  <default jcr:primaryType="jnt:folder">
                     <BANNER_heading.jpg j:height="1080"
                                         j:width="1920"
                                         jcr:mixinTypes="jmix:image"
                                         jcr:primaryType="jnt:file"
                                         jcr:title="WEB_SITE_OPTIONS_ORDEN">
                        <jcr:content jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                        <thumbnail jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                        <thumbnail2 jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                     </BANNER_heading.jpg>
                     <BANNER_provider.jpg j:height="1169"
                                          j:width="1920"
                                          jcr:mixinTypes="jmix:image"
                                          jcr:primaryType="jnt:file"
                                          jcr:title="WEB_SITE_OPTIONS_ORDEN">
                        <jcr:content jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                        <thumbnail jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                        <thumbnail2 jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                     </BANNER_provider.jpg>
                  </default>
               </themes>
            </files>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <base j:theme="#/modules/template-tutorial/templates/files/themes/default"
                  j:view="template-tutorial"
                  jcr:mixinTypes="jmix:theme"
                  jcr:primaryType="jnt:template">
               <home jcr:primaryType="jnt:pageTemplate">
                  <pagecontent jcr:mixinTypes="jmix:systemNameReadonly"
                               jcr:primaryType="jnt:contentList">
                     <layout-and-grid grid="12"
                                      jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:predefinedGrid"
                                      jcr:primaryType="bootstrap4nt:grid"
                                      rowCssClass="mb-4"
                                      rowHorizontalAlignment="default"
                                      rowVerticalAlignment="default"
                                      typeOfGrid="predefinedGrid"/>
                     <layout-and-grid-1 grid="4_4_4"
                                        jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:predefinedGrid"
                                        jcr:primaryType="bootstrap4nt:grid"
                                        rowHorizontalAlignment="default"
                                        rowVerticalAlignment="default"
                                        typeOfGrid="predefinedGrid"/>
                  </pagecontent>
               </home>
               <simple jcr:primaryType="jnt:pageTemplate">
                  <pagecontent jcr:mixinTypes="jmix:systemNameReadonly"
                               jcr:primaryType="jnt:contentList">
                     <layout-and-grid grid="4_8"
                                      jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:predefinedGrid"
                                      jcr:primaryType="bootstrap4nt:grid"
                                      rowHorizontalAlignment="default"
                                      rowVerticalAlignment="default"
                                      typeOfGrid="predefinedGrid"/>
                  </pagecontent>
               </simple>
               <pagecontent jcr:mixinTypes="jmix:systemNameReadonly"
                            jcr:primaryType="jnt:contentList">
                  <navbar addContainerWithinTheNavbar="true"
                          addLanguageButton="true"
                          addLoginButton="true"
                          divClass="collapse navbar-collapse"
                          jcr:mixinTypes="bootstrap4mix:advancedNavbar bootstrap4mix:brand"
                          jcr:primaryType="bootstrap4nt:navbar"
                          loginMenuULClass="navbar-nav flex-row ml-md-auto d-none d-md-flex"
                          navClass="navbar navbar-expand-lg navbar-light bg-light"
                          root="homePage">
                     <j:translation_en brandText="ACME"
                                       jcr:language="en"
                                       jcr:primaryType="jnt:translation"/>
                  </navbar>
                  <layout-and-grid grid="12"
                                   gridLayout="fixed-width"
                                   jcr:mixinTypes="bootstrap4mix:createContainer bootstrap4mix:createRow bootstrap4mix:predefinedGrid"
                                   jcr:primaryType="bootstrap4nt:grid"
                                   rowHorizontalAlignment="default"
                                   rowVerticalAlignment="default"
                                   typeOfGrid="predefinedGrid">
                     <layout-and-grid-main jcr:mixinTypes="jmix:systemNameReadonly"
                                           jcr:primaryType="jnt:contentList">
                        <breadcrumb jcr:mixinTypes="bootstrap4mix:advancedBreadcrumb"
                                    jcr:primaryType="bootstrap4nt:breadcrumb"/>
                        <pagecontent jcr:primaryType="jnt:area"/>
                     </layout-and-grid-main>
                  </layout-and-grid>
               </pagecontent>
            </base>
            <content-template j:applyOn="jnt:content"
                              j:hiddenTemplate="true"
                              j:priority="1"
                              j:view="template-tutorial"
                              jcr:primaryType="jnt:contentTemplate">
               <pagecontent jcr:primaryType="jnt:contentList">
                  <main-resource-display j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay"/>
               </pagecontent>
            </content-template>
         </templates>
         <home j:isHomePage="true"
               j:templateName="home"
               jcr:primaryType="jnt:page">
            <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
            <layout-and-grid-main jcr:mixinTypes="jmix:systemNameReadonly"
                                  jcr:primaryType="jnt:contentList">
               <carousel jcr:primaryType="bootstrap4nt:carousel">
                  <welcome-to-jahia image="#/modules/template-tutorial/templates/files/themes/default/BANNER_heading.jpg"
                                    jcr:primaryType="bootstrap4nt:carouselItem">
                     <j:translation_en caption="Cras justo odio, dapibus ac facilisis in, egestas eget quam. Donec id elit non mi porta gravida at eget metus. Nullam id dolor id nibh ultricies vehicula ut id elit."
                                       jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Welcome to Jahia"/>
                  </welcome-to-jahia>
               </carousel>
               <carousel-1 jcr:primaryType="bootstrap4nt:carousel">
                  <slide-2 image="#/modules/template-tutorial/templates/files/themes/default/BANNER_provider.jpg"
                           jcr:primaryType="bootstrap4nt:carouselItem">
                     <j:translation_en caption="Hello world"
                                       jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Slide 2"/>
                  </slide-2>
               </carousel-1>
            </layout-and-grid-main>
            <layout-and-grid-1-main jcr:mixinTypes="jmix:systemNameReadonly"
                                    jcr:primaryType="jnt:contentList">
               <rich-text jcr:primaryType="jnt:bigText">
                  <j:translation_en jcr:language="en"
                                    jcr:primaryType="jnt:translation"
                                    text="&lt;h2&gt;Heading&lt;/h2&gt;&#xA;&#xA;&lt;p&gt;Hello this is my first webpage and setting up templates.&lt;/p&gt;&#xA;"/>
               </rich-text>
            </layout-and-grid-1-main>
            <layout-and-grid-1-side jcr:mixinTypes="jmix:systemNameReadonly"
                                    jcr:primaryType="jnt:contentList">
               <rich-text jcr:primaryType="jnt:bigText">
                  <j:translation_en jcr:language="en"
                                    jcr:primaryType="jnt:translation"
                                    text="&lt;h2&gt;Heading&lt;/h2&gt;&#xA;&#xA;&lt;p&gt;Hello this is my first webpage and setting up templates.&lt;/p&gt;&#xA;"/>
               </rich-text>
            </layout-and-grid-1-side>
            <layout-and-grid-1-extra jcr:mixinTypes="jmix:systemNameReadonly"
                                     jcr:primaryType="jnt:contentList">
               <rich-text jcr:primaryType="jnt:bigText">
                  <j:translation_en jcr:language="en"
                                    jcr:primaryType="jnt:translation"
                                    text="&lt;h2&gt;Heading&lt;/h2&gt;&#xA;&#xA;&lt;p&gt;Hello this is my first webpage and setting up templates.&lt;/p&gt;&#xA;"/>
               </rich-text>
            </layout-and-grid-1-extra>
         </home>
      </template-tutorial>
   </modules>
</content>